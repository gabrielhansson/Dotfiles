! Rofi
rofi.font: monospace 12
rofi.padding: 10
rofi.line-padding: 2
rofi.color-enabled: true
rofi.hide-scrollbar: true
rofi.separator-style: solid
! background, border, search field bottom border
rofi.color-window: background, foreground, foreground
! alternating color, text color, alternating color, selected highlight
rofi.color-normal: background, foreground, background, foreground, background
rofi.color-urgent: argb:00000000, color1, argb:00000000, argb:00000000, color3
rofi.color-active: argb:00000000, color4, argb:00000000, argb:00000000, color3
rofi.kb-cancel: Escape
